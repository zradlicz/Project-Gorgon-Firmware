{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Pico Debug",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/build/sdi12-analog-mux.elf",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "openocd",
            "gdbPath": "gdb-multiarch",
            "preLaunchTask": "upload",
            "configFiles": [
                "interface/cmsis-dap.cfg",
                "target/rp2040.cfg"
            ],
            "searchDir": [],
            "openOCDPreConfigLaunchCommands": [
                "set USE_CORE 0"
            ],
            "openOCDLaunchCommands": [
                "adapter speed 5000"
            ],
            "svdFile": "${env:PICO_SDK_PATH}/src/rp2040/hardware_regs/rp2040.svd",
            "postRestartCommands": [
                "break main",
                "continue"
            ],
            "liveWatch": {
                "enabled": true,
                "samplesPerSecond": 4
            }
        },
        {
            "name": "Pico Attach",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/build/sdi12-analog-mux.elf",
            "request": "attach",
            "type": "cortex-debug",
            "servertype": "external",
            "gdbPath": "gdb-multiarch",
            "gdbTarget": "localhost:3333",
            "svdFile": "${env:PICO_SDK_PATH}/src/rp2040/hardware_regs/rp2040.svd"
        }
    ]
}
